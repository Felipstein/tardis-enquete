generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id
  accessToken  String  @map("access_token")
  refreshToken String  @map("refresh_token")
  expiresIn    Decimal @map("expires_in")

  @@map("users")
}
