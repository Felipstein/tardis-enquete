    "production": {
      "baseURL": "https://",
      "userAccessToken": "TEST_USER_ACCESS_TOKEN",
      "adminAccessToken": "TEST_ADMIN_ACCESS_TOKEN",
      "developerAccessToken": "TEST_DEVELOPER_ACCESS_TOKEN"
    }
  },
}
